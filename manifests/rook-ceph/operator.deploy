#!/bin/sh
set -o errexit -o nounset -o pipefail

# Run this script to deploy the rook-ceph operator to Kubernetes.

export release_name=operator
export release_namespace=rook-ceph
export chart_repo=https://charts.rook.io/release
export chart_name=rook-ceph
export values=$(dirname $0)/operator.values

exec $(dirname $0)/../../tools/helm-deploy.sh "$@"
